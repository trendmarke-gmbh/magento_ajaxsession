<?xml version="1.0"?>
<config>
  <modules>
    <Trendmarke_Ajaxsession>
      <version>0.1.0</version>
    </Trendmarke_Ajaxsession>
  </modules>
  <global>
    <helpers>
      <ajaxsession>
        <class>Trendmarke_Ajaxsession_Helper</class>
      </ajaxsession>
    </helpers>
  </global>
  <frontend>
    <routers>
      <ajaxsession>
        <use>standard</use>
        <args>
          <module>Trendmarke_Ajaxsession</module>
          <frontName>ajaxsession</frontName>
        </args>
      </ajaxsession>
    </routers>
  </frontend>
</config> 